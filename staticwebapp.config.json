{
  "auth": {
    "rolesSource": "DEFAULT"
  },
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 401
    }
  }
}
